{
  "name": "speed-tiptap-editor",
  "version": "0.1.0",
  "description": "基于 Ant Design Vue + Tiptap 的富文本编辑器",
  "main": "example/main.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:example",
    "build:lib": "cross-env BUILD_MODE=lib vite build",
    "build:example": "vite build --outDir dist-example",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:deploy": "npm run build:example && npm run docs:build && node scripts/prepare-deploy.js",
    "test": "vitest",
    "lint": "eslint src example --ext .ts,.tsx,.vue",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue}\" \"example/**/*.{ts,tsx,vue}\"",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "components",
    "ant-design-vue",
    "hooks",
    "utils"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/highlight.js": "^10.1.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.11.0",
    "@unocss/vite": "^0.58.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/compiler-sfc": "^3.4.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-vue": "^9.21.0",
    "fs-extra": "^11.3.0",
    "husky": "^9.0.0",
    "less": "^4.2.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "unocss": "^0.58.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.2.0",
    "vue-tsc": "^1.8.0"
